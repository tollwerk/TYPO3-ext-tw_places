{
    "name": "tollwerk/tw-places",
    "type": "typo3-cms-extension",
    "description": "Places and map search for TYPO3 websites",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Klaus Fiedler",
            "role": "Developer"
        }
    ],
    "version": "2.0.0",
    "require": {
        "typo3/cms-core": "^10.0",
        "tollwerk/tw-base": "^3.1.0",
        "tollwerk/tw-geo": "^1.4.0"
    },
    "autoload": {
        "psr-4": {
            "Tollwerk\\TwPlaces\\": "Classes",
        }
    },
    "replace": {
        "tw_places": "self.version",
        "typo3-ter/tw-places": "self.version"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:tollwerk/TYPO3-ext-tw_base.git",
            "reference": "master"
        },
        {
            "type": "vcs",
            "url": "git@github.com:tollwerk/TYPO3-ext-tw_geo.git",
            "reference": "master"
        }
    ]
}
